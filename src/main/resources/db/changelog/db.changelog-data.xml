<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <property name="initDataBase" value="false"/>
    <changeSet id="load_initial_data" author="Champion (generated)" context="${initDataBase}">
        <preConditions onFail="MARK_RAN">
            <and>
                <tableExists tableName="wallets"/>
                <sqlCheck expectedResult="0">SELECT COUNT(*) FROM wallets</sqlCheck>
            </and>
        </preConditions>
        <sqlFile path="data.sql" relativeToChangelogFile="true"/>
    </changeSet>
</databaseChangeLog>